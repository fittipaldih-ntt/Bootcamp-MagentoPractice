<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">images/logo.svg</argument>
                <argument name="logo_width" xsi:type="number">200</argument>
                <argument name="logo_height" xsi:type="number">100</argument>
                <argument name="logo_alt" xsi:type="string">Logo Test</argument>
            </arguments>
        </referenceBlock>

        <referenceContainer name="footer-bottom-content">
            <block class="Magento\Framework\View\Element\Template" name="custom-double-footer" template="Magento_Theme::custom_footer.phtml"/>
        </referenceContainer>

    </body>
</page>
